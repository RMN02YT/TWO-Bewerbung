{
    "extends": ["eslint:recommended", "google"],
    "env": {
		"node": true,
		"es6": true
	},
  "parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": 2021,
    "sourceType": "module",
    "requireConfigFile": false,
    "babelOptions": {
      "plugins": [
        "@babel/plugin-syntax-import-assertions"
      ]
    }
	},
    "rules": {
        "linebreak-style": ["error", "windows"],
        "max-len": ["off"]
    }
  }